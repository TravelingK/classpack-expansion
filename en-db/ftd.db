,"type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"type":{"value":"","subtype":""},"requirements":"","recharge":{"value":5,"charged":true}},"img":"modules/plutonium/media/icon/mailed-fist.svg","flags":{"plutonium":{"page":"monsterAction","source":"FTD","hash":"breath%20weapon%20%7b%40recharge%205%7d_moonstone%20dragon%20wyrmling_ftd_ftd"}},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"X4FtxkyhzYqEtHr0","name":"Breath Weapons: Dream Breath","type":"feat","system":{"description":{"value":"<div  class=\"rd__b  rd__b--3\"><p>The dragon exhales mist in a 90-foot cone. Each creature in that area must succeed on a DC <span class=\"rd__dc\">12</span> Constitution saving throw or fall @condition[unconscious] for 10 minutes. This effect ends for a creature if the creature takes damage or someone uses an action to wake it.</p><div class=\"rd__spc-inline-post\"></div></div>","chat":"","unidentified":""},"source":"FTD","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":90,"width":null,"units":"ft","type":"cone"},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"save","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"con","dc":12,"scaling":"wis"},"type":{"value":"","subtype":""},"requirements":"","recharge":{"value":null,"charged":false}},"img":"modules/plutonium/media/icon/mailed-fist.svg","flags":{"plutonium":{"page":"monsterAction","source":"FTD","hash":"breath%20weapons%3a%20dream%20breath_moonstone%20dragon%20wyrmling_ftd_ftd"}},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"fLktUyFL6ULdZmDA","name":"Breath Weapons: Moonlight Breath","type":"feat","system":{"description":{"value":"<div  class=\"rd__b  rd__b--3\"><p>The dragon exhales a beam of moonlight in a 30-foot line that is 5 feet wide. Each creature in that area must make a DC <span class=\"rd__dc\">12</span> Dexterity saving throw, taking 14 ([[/r 4d6]]) radiant damage on a failed save, or half as much damage on a successful one.</p><div class=\"rd__spc-inline-post\"></div></div>","chat":"","unidentified":""},"source":"FTD","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":30,"width":null,"units":"ft","type":"line"},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"save","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[["4d6","radiant"]],"versatile":""},"formula":"","save":{"ability":"dex","dc":12,"scaling":"wis"},"type":{"value":"","subtype":""},"requirements":"","recharge":{"value":null,"charged":false}},"img":"modules/plutonium/media/icon/mailed-fist.svg","flags":{"plutonium":{"page":"monsterAction","source":"FTD","hash":"breath%20weapons%3a%20moonlight%20breath_moonstone%20dragon%20wyrmling_ftd_ftd"}},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],"effects":[],"_stats":{"systemId":"dn